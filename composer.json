{
    "name": "felipe-trindade8/laravel-disk-monitor",
    "description": "Pacote teste",
    "keywords": [
        "felipe-trindade8",
        "laravel-disk-monitor"
    ],
    "homepage": "https://github.com/felipe-trindade8/laravel-disk-monitor",
    "license": "MIT",
    "authors": [
        {
            "name": "Felipe Trindade",
            "email": "felipe.trindade8@outlook.com",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/contracts": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "FelipeTrindade8\\LaravelDiskMonitor\\": "src",
            "FelipeTrindade8\\LaravelDiskMonitor\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FelipeTrindade8\\LaravelDiskMonitor\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "FelipeTrindade8\\LaravelDiskMonitor\\LaravelDiskMonitorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
